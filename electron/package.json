{
  "name": "LGP-1D-boilerplate",
  "version": "1.0.0",
  "description": "A minimal Electron and react boilerplate",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "webpack -d --watch",
    "test": "jest --coverage",
    "test-integration": "electron-mocha ./tests/integration",
    "build": "electron-packager . app --platform win32 --arch x64 --out dist/",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "electron": "^1.4.1",
    "electron-installer-windows": "^0.2.0",
    "electron-mocha": "^3.4.0",
    "electron-reload": "^1.1.0",
    "jest": "^19.0.2",
    "react-test-renderer": "^15.4.2",
    "spectron": "^3.6.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0"
  }
}
